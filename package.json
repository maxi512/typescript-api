{
  "name": "typescript-api",
  "version": "1.0.0",
  "description": "Setting up a repo with a simple API made with Typescript",
  "main": "index.ts",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint --ext ts,tsx .",
    "lint:fix": "eslint --ext ts,tsx . --fix",
    "test": "jest --reporters='default' --reporters='jest-junit'",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxi512/typescript-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maxi512/typescript-api/issues"
  },
  "homepage": "https://github.com/maxi512/typescript-api#readme",
  "dependencies": {
    "@types/supertest": "^2.0.12",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.17.3",
    "mongoose": "^6.3.1",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^17.0.25",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "jest-junit": "^13.2.0",
    "lint-staged": "^13.0.2",
    "nodemon": "^2.0.16",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.8.0",
    "typescript": "^4.6.3"
  }
}
